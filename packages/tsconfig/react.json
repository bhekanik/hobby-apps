{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"jsx": "react",
		"lib": [
			"es5",
			"es2015",
			"es2016",
			"dom",
			"esnext"
		],
		"module": "es2015",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"sourceMap": true,
		"strict": true,
		"target": "es6"
	}
}