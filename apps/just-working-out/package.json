{
	"name": "just-working-out",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"lint-staged": "lint-staged",
		"codegen": "contentful-typescript-codegen --output src/lib/generated/contentful.d.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-vercel": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/d3": "^7.4.0",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@xata.io/client": "^0.21.6",
		"autoprefixer": "^10.4.13",
		"contentful-management": "^10.26.0",
		"contentful-typescript-codegen": "^3.4.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.20",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-motion": "^0.11.5",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"lint-staged": {
		"src/**/*.{ts,tsx,svelte}": "eslint --cache --fix",
		"src/**/*.{ts,tsx,css,md,svelte}": "prettier --write",
		"src/**/*.{test,spec}.{ts,tsx,svelte}": "vitest related --run"
	},
	"dependencies": {
		"case": "^1.6.3",
		"contentful": "^9.2.14",
		"d3": "^7.8.0",
		"date-fns": "^2.29.3",
		"simple-statistics": "^7.8.0"
	}
}