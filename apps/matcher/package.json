{
  "name": "matcher",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "npm-run-all -p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "concurrently \"pnpm dev:css\" \"cross-env NODE_ENV=production netlify dev\"",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/vercel": "^1.8.2",
    "@types/react-dropzone": "^5.1.0",
    "@vercel/node": "^2.6.2",
    "@xata.io/client": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "cloudinary": "^1.33.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.16.7",
    "framer-motion": "^5.6.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-clamp-lines": "^3.0.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.7.1",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.4.5",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/node-persist": "^3.1.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.2.6",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "postcss": "^8.4.20",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --cache --fix",
    "src/**/*.{ts,tsx,css,md}": "prettier --write",
    "src/**/*.{test,spec}.{ts,tsx}": "vitest related --run"
  }
}